{
  "name": "buttercup",
  "version": "4.4.0",
  "description": "A NodeJS password vault.",
  "main": "dist/index.node.js",
  "types": "dist/index.d.ts",
  "contributors": [
    {
      "name": "Perry Mitchell",
      "email": "perry@perrymitchell.net"
    },
    {
      "name": "Sallar Kaboli",
      "email": "sallar.kaboli@gmail.com"
    }
  ],
  "scripts": {
    "analyse": "ANALYSE=bundle npm run build",
    "build": "run-s clean build:node build:web build:types",
    "build:node": "babel source --out-dir dist --minified --delete-dir-on-start --verbose",
    "build:node:test": "babel source --out-dir dist --delete-dir-on-start --verbose",
    "build:types": "./scripts/generate_dts.sh",
    "build:web": "webpack --mode=production",
    "clean": "rimraf ./dist && rimraf ./web",
    "dev": "concurrently -k 'npm run dev:node' 'npm run dev:web'",
    "dev:node": "babel source --out-dir dist --verbose --watch",
    "dev:web": "webpack --mode=development --watch",
    "docs": "npm run generate:docs",
    "format": "prettier --write '{source,test}/**/*.js'",
    "generate:docs": "jsdoc2md 'source/**/*.js' > API.md",
    "generate:vault": "npm run build:node && node scripts/generate_test_vault.js",
    "prepublishOnly": "run-s build",
    "test": "run-s test:node test:web test:format",
    "test:format": "prettier-check '{source,test}/**/*.js'",
    "test:integration": "npm run build:node:test && mocha -r test/index.js 'test/integration/**/*.spec.js'",
    "test:integration:watch": "nodemon --exec 'npm run test:integration'",
    "test:node": "npm run build:node:test && nyc mocha -r test/index.js 'test/{unit,integration}/**/*.spec.js'",
    "test:unit": "npm run build:node:test && mocha -r test/index.js 'test/unit/**/*.spec.js'",
    "test:unit:watch": "nodemon --exec 'npm run test:unit'",
    "test:web": "karma start",
    "test:web:production": "BUNDLE=production karma start"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "web/**/*.js",
    "web/**/*.d.ts",
    "*.md"
  ],
  "lint-staged": {
    "{source,test}/**/*.js": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercup/buttercup-core"
  },
  "keywords": [
    "password",
    "vault",
    "credentials",
    "login",
    "secure",
    "buttercup"
  ],
  "author": "Perry Mitchell <perry@perrymitchell.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buttercup/buttercup-core/issues"
  },
  "homepage": "https://github.com/buttercup/buttercup-core#readme",
  "dependencies": {
    "@buttercup/channel-queue": "^0.5.0",
    "@buttercup/dropbox-client": "^0.4.0",
    "@buttercup/googledrive-client": "^0.8.0",
    "cowl": "^0.8.0",
    "eventemitter3": "^4.0.4",
    "foreachasync": "^5.1.3",
    "fuse.js": "^2.7.4",
    "global": "^4.4.0",
    "gzip-js": "^0.3.2",
    "hash.js": "^1.1.7",
    "iocane": "^4.1.0",
    "is-promise": "^4.0.0",
    "js-base64": "^2.5.2",
    "url-join": "^4.0.1",
    "uuid": "^8.1.0",
    "verror": "^1.10.0",
    "webdav": "^3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@types/node": "^14.0.6",
    "arraybuffer-loader": "^1.0.8",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "husky": "^4.2.5",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jsdoc-to-markdown": "^5.0.3",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-mocha": "^2.0.1",
    "karma-sinon": "^1.0.5",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^10.2.7",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.1",
    "null-loader": "^4.0.0",
    "nyc": "^15.0.1",
    "pify": "^5.0.0",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "sleep-promise": "^8.0.1",
    "tmp": "^0.2.1",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11"
  }
}
